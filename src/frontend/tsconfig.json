{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "outDir": "./public/js",
        "rootDir": "./src/ts",
        "sourceMap": true,
        "lib": ["DOM", "DOM.Iterable", "ESNext"]
    },
    "include": ["src/ts/**/*"],
    "exclude": ["node_modules", "src/ts/*_old.ts"]
}